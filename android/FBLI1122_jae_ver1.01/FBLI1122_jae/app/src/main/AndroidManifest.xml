        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AccountActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AccountActivity.LoginActivity" />
        <activity android:name=".AccountActivity.SignupActivity" />
        <activity android:name=".AccountActivity.ResetPasswordActivity" />
        <activity android:name=".AccountActivity.BudgetManageActivity" />
        <activity android:name=".AccountActivity.GoDutchActivity" />
        <activity
            android:name=".AccountActivity.SetBudgetActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".AccountActivity.SetNameActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".AccountActivity.CommunityActivity" />
        <activity android:name=".AccountActivity.ChattingActivity" />
        <activity android:name=".AccountActivity.GroupMenuActivity" />
        <activity android:name=".AccountActivity.GroupActivity" />
        <activity
            android:name=".AccountActivity.MakeGroupActivity"
            android:theme="@android:style/Theme.Dialog" />
