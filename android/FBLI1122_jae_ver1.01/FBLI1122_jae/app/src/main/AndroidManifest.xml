        <activity android:name=".AccountActivity.testgroupActivity" />
        <activity
            android:name=".AccountActivity.ApplySpentActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".AccountActivity.SpentListActivity" />
        <activity android:name=".AccountActivity.EditSpentList" />
        <activity
            android:name=".AccountActivity.GroupItemPopupActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".AccountActivity.MessengerActivity" />
        <activity android:name=".AccountActivity.list_item" />
        <activity android:name=".AccountActivity.MyListAdpater" />
        <activity android:name=".AccountActivity.BoardDetailActivity" />
        <activity android:name=".AccountActivity.BoardPostActivity"></activity>
