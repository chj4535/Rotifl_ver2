<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">


    <Button
        android:id="@+id/manage_godutch"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/money_remain"
        android:layout_alignParentStart="true"
        android:layout_marginStart="80dp"
        android:layout_marginTop="80dp"
        android:text="GoDutch" />

    <Button
        android:id="@+id/set_budget"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/budget_view"
        android:layout_alignTop="@+id/budget_view"
        android:layout_marginEnd="-150dp"
        android:layout_marginTop="-10dp"
        android:text="SetBueget" />

    <TextView
        android:id="@+id/budget_view"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:textSize="20sp"
        android:layout_alignTop="@+id/textView3"
        android:layout_alignEnd="@+id/textView3"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="0dp"
        android:clickable="false"
        android:cursorVisible="false"
        android:gravity="right"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90dp"
        android:layout_marginTop="150dp"
        android:text="MyBudget"
        android:textSize="20dp" />
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView3"
        android:layout_alignTop="@+id/textView3"
        android:layout_marginTop="80dp"
        android:text="MoneySpent"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/spent_view"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:textSize="20sp"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignEnd="@+id/textView4"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="0dp"
        android:clickable="false"
        android:cursorVisible="false"
        android:gravity="right"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView4"
        android:layout_alignTop="@+id/textView4"
        android:layout_marginTop="80dp"
        android:text="MoneyRemain"
        android:textSize="20dp" />
    <TextView
        android:id="@+id/money_remain"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:textSize="20sp"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignEnd="@+id/textView5"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="0dp"
        android:clickable="false"
        android:cursorVisible="false"
        android:gravity="right"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="" />

    <Button
        android:id="@+id/apply_spent"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/spent_view"
        android:layout_alignTop="@+id/spent_view"
        android:layout_marginEnd="-150dp"
        android:layout_marginTop="-10dp"
        android:text="AddSpent" />

    <Button
        android:id="@+id/expense_list"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/money_remain"
        android:layout_marginEnd="80dp"
        android:gravity="center"
        android:layout_marginTop="80dp"
        android:text="Expense
List" />

</RelativeLayout>